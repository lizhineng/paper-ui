<view class="page">
  <view class="heading">
    <text>Icon</text>
  </view>

  <view
    wx:for="{{ icons }}"
    wx:key="category"
    class="session"
  >
    <view class="session-title">
      <text>{{ item.label }}</text>
    </view>

    <view class="demo flex">
      <x-button
        wx:for="{{ item.icons }}"
        wx:key="*this"
        text
      >
        <x-icon value="{{ item }}"/>
      </x-button>
    </view>
  </view>
</view>